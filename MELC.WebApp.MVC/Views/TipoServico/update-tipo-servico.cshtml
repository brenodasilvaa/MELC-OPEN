<div id="modalUpdateTipoServicoPlaceholder">
    <div class="modal fade" id="updateTipoServico" data-coreui-backdrop="static" data-coreui-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropAlert" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="staticBackdropAlert">Atualizar serviço</h5>
                    <button id="btnClose" type="button" class="btn-close" data-coreui-dismiss="modal" aria-label="Fechar"></button>
                </div>
                <div id="dlgUpdateTipoServico" class="modal-body">
                    <form class="row g-3">
                        <input hidden type="text" class="form-control" id="updateTipoServicoId" required>
                        <div class="col-mg-3">
                            <label for="servico" class="form-label">Serviço</label>
                            <input placeholder="Serra" type="text" class="form-control" id="updateServico" required>
                        </div>
                        <div class="col-mg-3">
                            <label for="preco" class="form-label">Preço por hora (R$)</label>
                            <input placeholder="R$" type="text" class="form-control" id="updatePreco" required>
                        </div>
                    </form>
                    <button style="margin-top:1rem" onclick="melc.tipoServico.atualizar()" id="updateTipoServicoBtn" class="btn btn-block btn-primary ">Atualizar</button>
                </div>
            </div>
        </div>
    </div>
</div>
